#include <stdio.h>
#include <stdlib.h>
#include <string.h>

#ifdef _MSC_VER
#define _CRT_SECURE_NO_WARNINGS
#endif


int convert()
{

	if (c >= '0' && c <= '9') {
		x = x * 10 + c - '0';
	}

}


int main() {

	FILE* input_f;
	input_f = fopen("in.txt", "r");
	char line[100];
  int sum=0;

	while (fgets(line, 100, input_f)){
		sum+=convert(line);
	}
	printf ("sum of numbers: %i \n", sum);
	fclose(input_f);
		return 0;

}
